<P ALIGN=CENTER><A NAME="lists"></A><TABLE BORDER=0 CELLPADDING=3
WIDTH="100%">
   <TR>
      <TH COLSPAN=4>
         <P><FONT SIZE=6>LISTS </FONT><HR SIZE="1" noshade>
      </TH><TH>
         <P ALIGN=LEFT>&nbsp;
      </TH></TR>
   <TR>
      <TD VALIGN=top width=40>
         <P>&nbsp;
      </TD><TD VALIGN=top width=140>
         <P><B>Unordered List</B>
      </TD><TD VALIGN=top width=240>
         <P><TT>&lt;UL&gt;&lt;LI&gt;&lt;/UL&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(<TT>&lt;LI&gt;</TT> before each list item)
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top ROWSPAN=2>
         <P>&nbsp;
      </TD><TD VALIGN=top ROWSPAN=2>
         <P><B>Bullet Type</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;UL TYPE=DISC|CIRCLE|SQUARE&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(for the whole list)
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR><TD VALIGN=top>
         <P><TT>&lt;LI TYPE=DISC|CIRCLE|SQUARE&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(this &amp; subsequent)
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>&nbsp;
      </TD><TD VALIGN=top>
         <P><B>Ordered List</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;OL&gt;&lt;LI&gt;&lt;/OL&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(<TT>&lt;LI&gt;</TT> before each list item)
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top ROWSPAN=2>
         <P>&nbsp;
      </TD><TD VALIGN=top ROWSPAN=2>
         <P><B>Numbering Type</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;OL TYPE=A|a|I|i|1&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(for the whole list)
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR><TD VALIGN=top>
         <P><TT>&lt;LI TYPE=A|a|I|i|1&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(this &amp; subsequent)
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top ROWSPAN=2>
         <P>&nbsp;
      </TD><TD VALIGN=top ROWSPAN=2>
         <P><B>Starting Number</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;OL START=?&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(for the whole list)
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR><TD VALIGN=top>
         <P><TT>&lt;LI VALUE=?&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(this &amp; subsequent)
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>&nbsp;
      </TD><TD VALIGN=top>
         <P><B>Definition List</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;DL&gt;&lt;DT&gt;&lt;DD&gt;&lt;/DL&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(<TT>&lt;DT&gt;=term, &lt;DD&gt;</TT>=definition)
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>&nbsp;
      </TD><TD VALIGN=top>
         <P><B>Menu List</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;MENU&gt;&lt;LI&gt;&lt;/MENU&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(<TT>&lt;LI&gt;</TT> before each list item)
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>&nbsp;
      </TD><TD VALIGN=top>
         <P><B>Directory List</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;DIR&gt;&lt;LI&gt;&lt;/DIR&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(<TT>&lt;LI&gt;</TT> before each list item)
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
</TABLE></P>

