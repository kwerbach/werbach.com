<P ALIGN=CENTER><A NAME="presentation"></A><TABLE BORDER=0
CELLPADDING=3 WIDTH="100%">
   <TR>
      <TH COLSPAN=4>
         <P><FONT SIZE=6>PRESENTATION FORMATTING </FONT><HR SIZE="1"
         noshade>
      </TH></TR>
   <TR>
      <TD VALIGN=top width=40>
         <P>&nbsp;
      </TD><TD VALIGN=top width=140>
         <P><B>Bold</B>
      </TD><TD VALIGN=top width=240>
         <P><TT>&lt;B&gt;&lt;/B&gt;</TT>
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>&nbsp;
      </TD><TD VALIGN=top>
         <P><B>Italic</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;I&gt;&lt;/I&gt;</TT>
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>N3.0b
      </TD><TD VALIGN=top>
         <P><B>Underline</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;U&gt;&lt;/U&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(not widely implemented yet)
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>&nbsp;
      </TD><TD VALIGN=top>
         <P><B>Strikeout</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;STRIKE&gt;&lt;/STRIKE&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(not widely implemented yet)
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>N3.0b
      </TD><TD VALIGN=top>
         <P><B>Strikeout</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;S&gt;&lt;/S&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(not widely implemented yet)
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>&nbsp;
      </TD><TD VALIGN=top>
         <P><B>Subscript</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;SUB&gt;&lt;/SUB&gt;</TT>
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>&nbsp;
      </TD><TD VALIGN=top>
         <P><B>Superscript</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;SUP&gt;&lt;/SUP&gt;</TT>
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>&nbsp;
      </TD><TD VALIGN=top>
         <P><B>Typewriter</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;TT&gt;&lt;/TT&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(displays in a monospaced font)
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>&nbsp;
      </TD><TD VALIGN=top>
         <P><B>Preformatted</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;PRE&gt;&lt;/PRE&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(display text spacing as-is)
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>&nbsp;
      </TD><TD VALIGN=top>
         <P><B>Width</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;PRE WIDTH=?&gt;&lt;/PRE&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(in characters)
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>&nbsp;
      </TD><TD VALIGN=top>
         <P><B>Center</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;CENTER&gt;&lt;/CENTER&gt;</TT>
         <B><A HREF="../barebones/barebone_annotation.html#align">[*]</A></B>
      
      </TD><TD VALIGN=top>
         <P>(for both text and images)
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>N1.0
      </TD><TD VALIGN=top>
         <P><B>Blinking</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;BLINK&gt;&lt;/BLINK&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(the most derided tag ever)
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>&nbsp;
      </TD><TD VALIGN=top>
         <P><B>Font Size</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;FONT SIZE=?&gt;&lt;/FONT&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(ranges from 1-7)
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>&nbsp;
      </TD><TD VALIGN=top>
         <P><B>Change Font Size</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;FONT SIZE="+|-?"&gt;&lt;/FONT&gt;</TT>
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>N1.0
      </TD><TD VALIGN=top>
         <P><B>Base Font Size</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;BASEFONT SIZE=?&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(from 1-7; default is 3)
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>&nbsp;
      </TD><TD VALIGN=top>
         <P><B>Font Color</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;FONT COLOR="#$$$$$$"&gt;&lt;/FONT&gt;</TT>
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>N3.0b
      </TD><TD VALIGN=top>
         <P><B>Select Font</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;FONT FACE="***"&gt;&lt;/FONT&gt;</TT>
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>N3.0b
      </TD><TD VALIGN=top>
         <P><B>Multi-Column Text</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;MULTICOL COLS=?&gt;&lt;/MULTICOL&gt;</TT>
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>N3.0b
      </TD><TD VALIGN=top>
         <P><B>Column Gutter</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;GUTTER=?&gt;</TT>
      </TD><TD VALIGN=top>
         <P>(default is 10 pixels)
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>N3.0b
      </TD><TD VALIGN=top>
         <P><B>Column Width</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;WIDTH=?&gt;</TT>
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>N3.0b
      </TD><TD VALIGN=top>
         <P><B>Spacer</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;SPACER&gt;</TT>
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>N3.0b
      </TD><TD VALIGN=top>
         <P><B>Spacer Type</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;SPACER TYPE=horizontal|vertical|block&gt;</TT>
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>N3.0b
      </TD><TD VALIGN=top>
         <P><B>Spacer Dimensions</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;SPACER SIZE=?&gt;</TT>
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>N3.0b
      </TD><TD VALIGN=top width140>
         <P><B>Spacer Dimensions</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;SPACER WIDTH=? HEIGHT=?&gt;</TT>
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
   <TR>
      <TD VALIGN=top>
         <P>N3.0b
      </TD><TD VALIGN=top>
         <P><B>Spacer alignment</B>
      </TD><TD VALIGN=top>
         <P><TT>&lt;SPACER ALIGN=left|right|center&gt;</TT>
      </TD><TD VALIGN=top>
         <P>&nbsp;
      </TD></TR>
</TABLE></P>

